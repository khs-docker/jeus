<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE page-specification PUBLIC "-//Apache Software Foundation//Tapestry Specification 4.0//EN" "http://jakarta.apache.org/tapestry/dtd/Tapestry_4_0.dtd">
<page-specification class="jeus.tool.webadmin.page.PermanentModule.OverviewPage">
    <component id="mainBorder" type="jeus:MainBorder">
        <binding name="title" value="literal:Permanent Module Overview"/>
    </component>

    <component id="applicationForm" type="jeus:JEUSForm">
        <binding name="title" value="message:pm.overview.title"/>
        <binding name="useDefaultButtons" value="false"/>
    </component>

    <component id="tabPanel" type="jeus:TabPanel">
        <binding name="pageNames" value="{'PermanentModule_Overview', 'PermanentModule_Configuration'}"/>
    </component>

    <component id="applicationContent" type="Insert">
        <binding name="value" value="application"/>
    </component>

</page-specification>
