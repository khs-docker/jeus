<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE page-specification PUBLIC "-//Apache Software Foundation//Tapestry Specification 4.0//EN" "http://jakarta.apache.org/tapestry/dtd/Tapestry_4_0.dtd">
<page-specification class="jeus.tool.webadmin.page.JeusMonitoringMBeanMonitoringService.OverviewPage">
   
    <component id="popupMainBorder" type="jeus:PopupMainBorder">
        <binding name="title" value="literal:Result"/>
    </component>

    <property name="currentStats"/>                          
    <asset name="form_css" path="context:/css/form.css"/>

    <component id="mbean_monigoring" type="Image">
        <binding name="image" value="ognl:assets.mbean_monigoring"/>
    </component>

    <component id="description_top" type="Image">
        <binding name="image" value="ognl:assets.description_top"/>
    </component>

    <component id="description_bottom" type="Image">
        <binding name="image" value="ognl:assets.description_bottom"/>
    </component>

    <asset name="mbean_monigoring" path="context:/image/mbean/mbean_monigoring.gif"/>
    <asset name="description_top" path="context:/image/log/white_background2.gif"/>
    <asset name="description_bottom" path="context:/image/log/white_background1.gif"/>

</page-specification>
