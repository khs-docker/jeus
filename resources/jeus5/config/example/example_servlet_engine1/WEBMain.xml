<?xml version="1.0" encoding="UTF-8"?>
<web-container xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <monitoring>
        <check-thread-pool>300000</check-thread-pool>
        <check-class-reload>300000</check-class-reload>
        <check-session>300000</check-session>
    </monitoring>
    <context-group>
        <group-name>MyGroup</group-name>
        <webserver-connection>
            <http-listener>
                <listener-id>http1</listener-id>
                <port>8088</port>
                <thread-pool>
                    <min>10</min>
                    <max>20</max>
                    <step>1</step>
                    <max-idle-time>300000</max-idle-time>
                    <max-wait-queue>4</max-wait-queue>
                    <max-queue>-1</max-queue>
                </thread-pool>
                <back-log>50</back-log>
                <server-access-control>false</server-access-control>
            </http-listener>
        </webserver-connection>
        <session-config>
            <shared>false</shared>         
        </session-config>
        <jsp-engine>
            <keep-generated>true</keep-generated>
            <check-included-jspfile>false</check-included-jspfile>
        </jsp-engine>
    </context-group>
    <session-cluster>
        <session-routing>false</session-routing>
        <session-server>
            <server-name>session1</server-name>
            <connect-timeout>60000</connect-timeout>
            <read-timeout>120000</read-timeout>
        </session-server>
    </session-cluster>
</web-container>
