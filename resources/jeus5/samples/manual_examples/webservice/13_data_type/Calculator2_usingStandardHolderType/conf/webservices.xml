<?xml version="1.0"?>
<webservices version="1.1" xmlns="http://java.sun.com/xml/ns/j2ee">
    <webservice-description>
        <webservice-description-name>Calculator2Service</webservice-description-name>
        <wsdl-file>WEB-INF/wsdl/Calculator2Service.wsdl</wsdl-file>
        <jaxrpc-mapping-file>WEB-INF/Calculator2Service-mapping.xml</jaxrpc-mapping-file>
        <port-component>
            <port-component-name>CalculatorIFPort</port-component-name>
            <wsdl-port xmlns:ns2="urn:Calculator2Service">ns2:CalculatorIFPort</wsdl-port>
            <service-endpoint-interface>calc.CalculatorIF</service-endpoint-interface>
            <service-impl-bean>
                <servlet-link>Calculator2Servlet</servlet-link>
            </service-impl-bean>           
        </port-component>
    </webservice-description>
</webservices>

