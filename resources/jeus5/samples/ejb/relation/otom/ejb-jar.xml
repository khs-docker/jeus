<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <session>
      <display-name>otmtest</display-name>
      <ejb-name>otmtest</ejb-name>
      <home>ejb.relation.otom.otmtestHome</home>
      <remote>ejb.relation.otom.otmtest</remote>
      <ejb-class>ejb.relation.otom.otmtestBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    <entity>
      <display-name>teacher</display-name>
      <ejb-name>teacher</ejb-name>
      <local-home>ejb.relation.otom.TeacherHome</local-home>
      <local>ejb.relation.otom.Teacher</local>
      <ejb-class>ejb.relation.otom.TeacherBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>ejb.relation.otom.TeacherPK</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>teacher</abstract-schema-name>
      <cmp-field>
        <field-name>name</field-name>
      </cmp-field>
      <cmp-field>
        <field-name>teacherid</field-name>
      </cmp-field>
    </entity>
    <entity>
      <display-name>student</display-name>
      <ejb-name>student</ejb-name>
      <local-home>ejb.relation.otom.StudentHome</local-home>
      <local>ejb.relation.otom.Student</local>
      <ejb-class>ejb.relation.otom.StudentBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>student</abstract-schema-name>
      <cmp-field>
        <field-name>studentid</field-name>
      </cmp-field>
      <cmp-field>
        <field-name>name</field-name>
      </cmp-field>
      <primkey-field>studentid</primkey-field>
    </entity>
  </enterprise-beans>
  <relationships>
    <ejb-relation>
      <ejb-relation-name>student-teacher</ejb-relation-name>
      <ejb-relationship-role>
        <description>student</description>
        <ejb-relationship-role-name>studentRelationshipRole</ejb-relationship-role-name>
        <multiplicity>Many</multiplicity>
	<cascade-delete/>
        <relationship-role-source>
          <description>student</description>
          <ejb-name>student</ejb-name>
        </relationship-role-source>
        <cmr-field>
          <description>teacher</description>
          <cmr-field-name>teacher</cmr-field-name>
        </cmr-field>
      </ejb-relationship-role>
      <ejb-relationship-role>
        <description>teacher</description>
        <ejb-relationship-role-name>teacherRelationshipRole</ejb-relationship-role-name>
        <multiplicity>One</multiplicity>
        <relationship-role-source>
          <description>teacher</description>
          <ejb-name>teacher</ejb-name>
        </relationship-role-source>
        <cmr-field>
          <description>student</description>
          <cmr-field-name>student</cmr-field-name>
          <cmr-field-type>java.util.Collection</cmr-field-type>
        </cmr-field>
      </ejb-relationship-role>
    </ejb-relation>
  </relationships>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>teacher</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>otmtest</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>student</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
