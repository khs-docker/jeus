<?xml version="1.0" encoding="UTF-8"?>
<xs:schema 
    attributeFormDefault="unqualified" 
    elementFormDefault="qualified" 
    targetNamespace="http://www.tmaxsoft.com/xml/ns/jeus" version="5.0" 
    xmlns="http://www.tmaxsoft.com/xml/ns/jeus" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema">       
    <xs:element name="ws-engine" type="ws-engineType">         
        <xs:annotation>             
            <xs:documentation><![CDATA[
                [Description]: This is the top-level XML element for configuring JEUS Web Server
            ]]>
            </xs:documentation>         
        </xs:annotation>     
    </xs:element>      
    <xs:complexType name="ws-engineType">         
        <xs:sequence>             
            <xs:element name="domain" type="domainType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This elelment describes the configuration for JEUS Web Server System.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="domainType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: It sets the domain name.
                    		       The domain name will be encoded along with the host name to be used for license verification process.
                        [Value Description]: String type with no more than 31 characters.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="1" name="node" type="nodeType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Set the configuration options of the nodes that compose a Web Server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="vhost" type="vhostType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Configure the virtual hosting on Web server.
                                                Virtual Host refers to the function of providing each different service via each different URL, and so even though only one server is running, 
                                                but it looks like that multiple servers are running for a service.                                        
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="1" name="svrgroup" type="svrgroupType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: When accessing to server processes via Web Server, it needs to divide the server processes into groups based on their logical relationship.
                                                This is the item to specify group options.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="1" name="server" type="serverType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Register the services which are to be provided.
                                                Web server processes only the registered services, therefore you must register the service for the newly added server program.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="service" type="ws-serviceType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Necessary to specify this option only when directly executing the business logic via Web server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="directory" type="directoryType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the characteristic of particular directory in node.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="uri" type="uriType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: This item enables the server to identify the processing service for each URI(Uniform Resource Identifier)value of client request.
                                                In other words, if a particular URI comes in, the specified service will be assigned to process the request with the URI.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="alias" type="ws-aliasType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the physical directory path-to-URI 'Alias' in the server.
                                                In other words, the incoming request for the certain URI is routed to the physical directory in basis of the alias rule, then processed by using the resources in physical directory.
                                                User can specify the 'alias' regardless of document root, thereby obtaining greater efficiency in administering the system.
                                                However, the physical directories must be set to absolute path.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="dirindex" type="dir-indexType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: This automatically lists the entire directory structure to client, when client requests particular file such as index.html but such file does not exist.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="logging" type="ws-loggingType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the record type of client request history. The access and error history are individually saved.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="access" type="accessType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Used to limit or grant the access authority over the resources in the sever according to client IP.
                                                Access section can apply to DIRECTORY/URI/EXT section, and will limit or allow the access over the resources defined by each section.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="authent" type="authentType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the authentication procedure for controlling the client access by user and group.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="ext" type="extType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]:	This item appoints the process to each client request based on the requested file extension.
                                                Generally the process for each default MIMI-TYPE has already been specified, However in case of additional configuration, you can use this section.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="ssl" type="sslType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the SSL function to be used in Web server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="errordocument" type="error-docType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Followings are 4 types of actions to be taken when an error occurred in Web Server.
	                                            1. Print out the error message defined in source code.
	                                            2. Print out user-defined error message.
                                              3. Forward it to local URL.
                                              4. Forward it to external URL.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="filter" type="filterType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: This item enables modifying (filtering) of request or response 
                                       just prior to processing the client HTTP request or sending the HTTP response to the client.
                        [Hidden]: true                                                
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="expires" type="expiresType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the information of server response headers.
                                                When transmitting the certain MIME type content, this option specifies the expiry date of the content transmitted in the server response header. 
                                                When client browser connects to the previously visited web site, if the web pages are stored in browser cache,
                                                then browser will download only the updated contents. In this case, the client browser submits a request to Web server for sending back only the expired contents,
                                                while sending the particular MIME type and the expirty date of the MIME.
                                                Web server analyzes the MIME-TYPE and expiry date contained in the client request header, 
                                                and if the expiry date of MIME-TYPE page exceeds the current date, Web server will then send it back to client. 
                                                The expiry date ,which client sent to Web server, is the one that Web sever had sent to client by referring to the configuration file.
                                                The 'expires item' defines the things relevant to the this expiry date setting.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="tcpgw" type="tcpgwType">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <section>true</section>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the listener and the server for processing client request. 
                                                the tcpgw listenes on the specified port, and if data comes in, it will send the data to the specified server.
                                                For instance, the tcpgw in the Web server sends the data to the tcp-servlet in JEUS by round-robin manner.
                                                In case of the port limitation due to the firewall, you can use the WebtoB server as proxy server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="30" minOccurs="0" name="nlive-inq" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the hearbeat check period (in seconds) between multiple web servers.
                                       If the time is set too short, there can be overhead in the network due to frequent heartbeat checks.
                                       If the time is set too long, the status of servers cannot be identified quickly.
                                       This value must be set by considering the network volume and importance of the web service.
                        [Value Description]: second
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="nodeType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the physical node name (host name).
                                       Host name can be found by 'uname -n' command in UNIX.
                                       A logical name can be used and the node name does not need to be listed in 'etc/hosts' file.  However the logical name must be same as the one configured in Nodename element.
                                       One or more node name must be configured.         
                        [Value Description]: String with up to 31 characters
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="webtob-dir" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the absolute path of JEUS Web Server directory.
                                       This path must be same as the value of environmental variable of WEBTOBDIR.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="shmkey" type="xs:long">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the value for shared memory segment which is used by JEUS web server processes.
                        		Before this configuration, make sure this key is not used by other programs.  If there is conflict, JEUS Web Server will not boot up.  
                        [Value Description]: The value can be anywhere between 32768 and 262143.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="docroot" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the absolute path of the root directory that contains all served contents.
                                       JEUS Web Server will find the requested content based on this root directory.  The requested URI will be appended on this path to indicate the absolute path of the requested content.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="tcpgw-name" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>tcpgw</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of TcpGW
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="hostname" type="len128Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the host name field of Http Response Header.
                        [Example]: www.tmaxsoft.com
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="nodename" type="len128Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the Host Name of the server.  If it is set to $(NODENAME), JEUS Web Server will automatically assigns the appropriate host name.
                        [Value Description]: Korean fonts and spaces are allowed.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="1" minOccurs="0" name="hth" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the number of HTH (HTTP Request Handler) Process.
                                       HTH is the intermediate process in between a client browser and a service process of JEUS Web Server.
                                       HTH receives and transferes client requests to appropriate service processes.  It receives results from service processes and sends them to clients.        
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="7777" minOccurs="0" name="node-port" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the port number used for inter-communication of nodes. 
                                       This port number must be configured for multiple JEUS web server system.         
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="jsv-port" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the port number used for communcation between JEUS Web Server and Java Servlet Process.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="3333" minOccurs="0" name="rac-port" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the port number used for management process 'wsracd' daemon in multiple web server system.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="30000" minOccurs="0" name="client-check-interval" type="xs:long">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>clichkintval</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify this option to check whether the clients connecting to WebtoB Server are alive or not.
                                                WebtoB automatically checks the client at the specified intervals, and if the client is dead, the connection will be automatically terminated by Web server.
                        [Value Description]: Milisecond unit
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="cache-use" type="cache-useType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the cache service of web server.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="max-user" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of connected users.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="txlog-dir" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the directory path for transaction logs. The value can be either an absolute path or a relative path based on WEBTOBDIR.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="syslog-dir" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the directory path for system logs.  The value can be either an absolute path or a relative path based on WEBTOBDIR.
                                       It is only used for redefining the system log directory.  If this element is not configured, the system logs will be stored in Default Path.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="language-priority" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>languageprio</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: When connected client does not have specified language, this item specifies the priority of processing multiview request according to the language specified in the server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0700" minOccurs="0" name="ip-permission" type="xs:int">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>ipcperm</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: IPCPERM(inter-process communication permission mask) enables users, not an administrator, to conduct wsdown, start and shutdown of the particular process, 
                                                and even check the result via wsadmin.
                                                In Unix environment, you can give different level of authority  to admin, individual users, groups, etc by setting up the file access control (view/record/execute).
                                                In other words, if set to default, users are not able to use above functions, but if IPCPERM is set to 0777, user can use all the functions.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="511" minOccurs="0" name="listen-backlog" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the size of request queue.  This option is not normally used.  
                        	       It is not a commonly used configuration, however it can be a valuable feature if the web server is receving large number of requests at once.  (FYI, hackers often attacks a web site by sending large number of requests at once)
                                       The requests will be rejected if the size of queue reaches this value.         
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="send-buffer-size" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the TCP buffer size.  For some environment, this configuration can enhance the performance.
                        [Defined Value]: [0] : means the default value of the operating system.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="limit-request-body" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the request body size (in bytes) of a HTTP client request.
                        [Defined Value]: [0] : means unlimited.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="100" minOccurs="0" name="limit-request-fields" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of HTTP request header fields.
                        [Value Description]: The number of HTTP request header fields
                        [Defined Value]: [0] : means unlimited.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="8190" minOccurs="0" name="limit-request-fieldsize" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum size (in bytes) of a HTTP Request header field.
                        [Value Description]: bytes
                        [Defined Value]: The default value is 8190 which is the largest value allowed.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="8190" minOccurs="0" name="limit-request-line" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum size (in bytes) of a HTTP Request Line.
                        [Value Description]: bytes
                        [Defined Value]: The default value is 8190 which is the largest allowed value.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="MIN" minOccurs="0" name="server-tokens" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the handling method of the server information in HTTP Response Header Server.
                        [Defined Value]: [Off] : No server information is sent.
                                         [Prod(uctOnly)] : ex. WebtoB
                                         [Min(imal)] : ex. WebtoB/3.1.5
                                         [OS] : ex. WebtoB/3.1.5 (LINUX_i386)
                                         [Full] : ex. WebtoB/3.1.5 (LINUX_i386)
                                         [Custom=xxx/x.x] : ex. xxx/x.x
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="6666" minOccurs="0" name="ipc-base-port" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the port number for IPC between internal processes.
                                       This option is only supported in Windows.  For previous versions, this port number was set up in the environment variable of 'WEBTOB_WINDOWS_PORT'.
                                       If the port number is set in both places, the value in the environment variable has the precedence.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="user" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the operating system user who can operate the web server.
                        	       It is not supported in Windows platforms.  Setting the root account is not recommended.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="group" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the operating system group who can operate the web server.
                        	       It is not supported in Windows platforms.  Setting the root account is not recommended.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="admin" type="len128Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the administrator's profile such as his/her email address.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="method" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the client request method.
                                       The default configuration supports GET, POST, and HEAD and you can disable a method by using ? Option.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="port" type="len1024Type">                 
                <xs:annotation>                     
