                    <xs:documentation><![CDATA[
                        [Description]: This element sets the database name.  
                        	       This element is used when the server group has TMS configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="open-info" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures how to open a database connection.  It uses the command supported by the target database.
                        	       This element is used when the server group has TMS configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="close-info" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures how to open a database connection.  It uses the command supported by the target database.
                        	       This element is used when the server group has TMS configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="min-tms" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the minimum number of TMS processes. This element is used when the server group has TMS configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="max-tms" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the minimum number of TMS processes. This element is used when the server group has TMS configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="tms-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This elements sets the TMS process name to configure TMS for the server group.
                        		TMS, a XA transaction manager, is required when the server group needs access to databases.
                                        TMS process is created using $TMAXDIR/lib/libtms.a and DBNAME in SVRGROUP element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="env-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the environment variable file that is needed when the server group is sharing the environment variables with its assigned servers or interoperating with multiple databases from same vendor in the same node.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="authent-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the authentication for the server group. 
                                       In order to use authentication on the server group, set the authent name in authent element in this element.  The user ID and password must be configured using wsmkpw utility.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="logging" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the logging.  The value must be one of the names of the ws-loggingType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="script-location" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>scriptloc</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: When specifying the php-related server group, this option specifies the path where php execution modules are actually located.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="filter-name" type="xs:token">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>filter</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify the filter by which HTTP request and HTTP response are to be modified(filtered).
                        [Hidden]: true
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>    
            <xs:element minOccurs="0" name="default-charset" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the character set to apply when a HTTP header is missing charcter set setting. 
				       The precedence of the configuration ordered from highest to lowest is Directory, SVRGROUP, VHOST, and Node.
						[Defined Value]: [On] : Uses the standard character set iso-8859-1.
										[Off] : Disables this feature.
										[_charset_] : Uses the specified character set.  e.g. "UTF-8"
		    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="default-mimetype" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the default MIMETYPE to be used when a MIMETYPE of a requested resource is not recognizable.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="expires-name" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>expires</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specify expiry options in Web Server.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="serverType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the server name.  The name must be unique within the server element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svg-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the server group that the server belongs to.
                                       This value must be one of the server group name configured in the svrgroup element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="cl-opt" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This elelemt sets the commands for server process initiation.
                                       The options defined before '--' are used by the system and the options defined after '--' are used by users.
                        [Defined Value]: [-e] : -e [file name] It records the standard errors in the file.  A file named [server process name]_[file name] is created to log the standard errors occurred in the server process.
                                         [-o] : -o [file name] It records the standard output in the file.  A file named [server process name]_[file name] is created to log the standard outputs occurred in the server process.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="min-proc" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the minum number of server processes.
                        	       JEUS Web Server maintains a certain number of server processes and controls the size within the min process # (min-proc) and the max process # (max-proc) for optimal performance.  
                        	       JEUS Web Server will initially start this minimul number of server processes at booting time.
                        	       The performance can be enhanced by assigning the appropriate # of server processes for certain services.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="max-proc" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of server processes.
                               	       JEUS Web Server maintains a certain number of server processes and controls the size within the min process # (min-proc) and the max process # (max-proc) for optimal performance.  
                        	       JEUS Web Server can increase the # of server processes up to this value. 
                        	       The performance can be enhanced by assigning the appropriate # of server processes for certain services. 
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="userlog-dir" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the user log directory.  The path can be either absolute or relative based on WEBTOBDIR.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="uri-dir" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: 
                        		This element specific URIs to serve.
                                        If a specific URI is requested, this server will provide the service for the URI.
                                        High priority services can be configured here to have the dedicated servers for high performance.
                                        This value must be one of the names in the URIType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="maxq-count" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the size of request queue.  This option is not normally used.  
                        	       However it can be a valuable feature if the web server is receving large number of requests at once.  (FYI, hackers often attacks a web site by sending large number of requests at once)
                                       The requests will be rejected if the size of queue reaches this value.          
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="asq-count" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the number of requests on queue as a threshold to activate an additional server process.
                                       If the queue reaches this size, an additional server process will be started as long as the number of the server processes are less than max-proc value.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="10" minOccurs="0" name="max-restart" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of times to restart the server process.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="1" minOccurs="0" name="svr-cpc" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the number of HTH processes and channels for parallel communication.
                                       If a server process slows down due to processing a large volume of requests, setting up the parallel communication can enhance the performance.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svr-type" type="svrElementType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures what type of the service that the server provides.
                                       The server type can be HTML, CGI, JSV, WEBSTD, TPSTD, SSI, and etc.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="4096" minOccurs="0" name="http-out-buf-size" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the buffer size of a response.
                                       Set this value as 0 to use the caching feature.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="8192" minOccurs="0" name="http-in-buf-size" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the buffer size of a request.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="max-requests" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of requests that this server can service before rebooting.
                                       If the server processes this number of requests, it gets automatically rebooted.
                                       If an application running on the web server has a bug in memory management, this feature can be used to reset the memory.
                                       If client requests require continuous service, the server may process more than this number before rebooting.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="ws-serviceType">         
        <xs:sequence>             
            <xs:element name="name" type="len16Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the service routine name to be used in JEUS Web Server.
                        [Value Description]: String within 15 characters.  The value must be unique in ws-serviceType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="50" minOccurs="0" name="priority" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the priority of processing.
                                       The value can be set in between 1 and 50, and a higher number gets a higher priority.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="svc-time" type="xs:long">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum service time.
                                       If a service takes more than this configured time, it is stopped and the server sends an error to the client.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="directoryType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of the directory.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="dir-path" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>directory</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element setsthe directory path using either an absolute or relative path based on WEBTOBDIR.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>    
            <xs:element minOccurs="0" name="default-charset" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the character set to apply when a HTTP header is missing charcter set setting. 
				       The precedence of the configuration ordered from highest to lowest is Directory, SVRGROUP, VHOST, and Node.
						[Defined Value]: [On] : Uses the standard character set iso-8859-1.
										[Off] : Disables this feature.
										[_charset_] : Uses the specified character set.  e.g. "UTF-8"
		    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="default-mimetype" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the default MIMETYPE to be used when a MIMETYPE of a requested resource is not recognizable.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="force-mimetype" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the MIMETYPE to be applied to all resources in the directory.
                                       If force-mimetype is set to CGI, all resources in this directory will be processed as CGI.  
                                       Therefore all client requests will be serviced by the web server whose server type is CGI.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="uriType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="uri-def" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>uri</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Enter URI for the value of service attribute.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="svr-type" type="svrElementType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures what type of the service that the server provides.
                                       The server type can be HTML, CGI, JSV, WEBSTD, TPSTD, SSI, and etc.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svr-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the server name that processes the URI.
                        	       It is required if WBAPI is used and the value must be one of the WBAPI servers registered in serverType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svc-name" type="len16Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the service name of TP service or WBAPI service to use.
                        	       Only required when TP Service or WBAPI service is used.        
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="redirect" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the target URI to redirect the request to.  
                                       The server puts the Redirect Status code as the loacation header of the HTTP response when a request is redirected.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="redirect-status" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the HTTP status when a request is redirected. This value will be set in the location header of HTTP response.
                        [Defined Value]: If not configured, 'found(302)' is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="vhost-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the URI for a specific vhost.  
                        	       e.g. If all vhosts has the same URI but want to use different SVRTYPE then this element can be configured.
                                       This value must be one of the vhost configured in vhostType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="ws-aliasType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="uri-def" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>uri</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Enter URI for the value of service attribute.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="real-path" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the absolute path for the alias directory.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svr-type" type="svrElementType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the server type to process a client request for the alias.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="node-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the node where the alias will be applied.
                        	       So the alias can be another node's directory.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="vhost-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the vhosts where the alias will be applied.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="dir-indexType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="option" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the options for indexing.
                        [Defined Value]: [Fancy]: It enables the fancy indexing feature. A column title can be clicked to sort. If clicked again, the sort order is changed. If 'FANCY' is put in this element, it is enabled.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="ignore" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the files that should not be listed for indexing.
                                       The file names to be ingnored should be listed. Wildcard(*) can be used in the file name.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="default-icon" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the icon to be used for unknown file types.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="description" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets a description for a specific file.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="header-file" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the file that contains the header to be placed at the top of the indexing.
                                       The file path must be set relative to the indexed directory.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="tail-file" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the file that contains the header to be placed at the bottom of the indexing.
                                       The file path must be set relative to the indexed directory.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="icon-ext" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the mapping between file extensions and icon image files.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="ws-loggingType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="format" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures the content and record method.
                                       If format is not configured, the default log format is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="file-name" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the file path and name for logging.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="1" minOccurs="0" name="valid-days" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]:  This element decides whether logs will be stored in a file or daily log will be created as a separate file.
                        [Defined Value]: [1] :  daily log files are created (each file will be created at 12:00 AM
                                         [0 or disable] : logs will be stored in a file.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="option" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the option for logging.
                        [Defined Value]: [Sync]: The server writes the logs on the file without buffering. User logs can be read from the file without any delay. It may reduce the performance due to very frequent I/O task.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="accessType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of access.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="deny, allow" minOccurs="0" name="order" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the precedence between allow and deny.
                        [Value Description]: {"deny,allow"|"allow,deny"|"mutual-failure"}
                        [Defined Value]: [deny, allow]: deny is applied first and then allow is applied.  Unmatched client requests are allowed.
                                         [allow, deny]: allow is applied first and then deny is applied.  Unmatched client requests are not allowed.
                                         [mutual-failure]: Clients that are in the allow list and not in the deny list are allowed.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="allow" type="len1024Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the allowed IP addresses and network/netmask.
                        [Value Description]: {"all"|"{ipaddr|ipaddr/netmask},..."}
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="deny" type="len1024Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the denied IP addresses and network/netmask.
                        [Value Description]: {"all"|"{ipaddr|ipaddr/netmask},..."}
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="authentType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="type" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the authorization control method.  WebtoB currently supports only the basic type.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="user-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the file that stores user IDs and passwords.
                                       JEUS Web server provides 'wsmkpw' utility that can be used to set user IDs and passwords.
                                       'wsmkpw' utility stores the user IDs and passwords (encrypted) in the file.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="group-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the group authorization information file.
                                       The file lists the groups and the assigned users.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="extType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="mimetype" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the MIME type to assign the specific server types.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="HTML" minOccurs="0" name="svr-type" type="svrElementType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the sever type to process the previously configured 'mimetype'.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="svr-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the server name to process the previously configured 'mimetype'. 
                        	       It can be used for assigning a specific server to service a specific mime type.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="charset" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the character set for the 'mimetype'.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="sslType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name for the ssl setting.  String value is used.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="certificate-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the PEM-encoded certificate file.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="certificate-key-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the private key file for the certificate.
                                       Generally, the file is stored in the SSL directory of JEUS Web Server.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="CA-certificate-path" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the directory where you keep the Certificates of Certification Authorities (CAs) whose clients you deal with. 
                        	       These are used to verify the client certificate on Client Authentication.
                                       The certificates should be PEM-encoded.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="CA-certificate-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This directive sets the all-in-one file where you can assemble the Certificates of Certification Authorities (CA) whose clients you deal with.
                        	       These are used for Client Authentication. 
                        	       Such a file is simply the concatenation of the various PEM-encoded Certificate files, in order of preference. 
                        	       This can be used alternatively and/or additionally to CA-certificate-path. 
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="certificate-chain-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the directory path to store Certificate Agent's certificates for Certification Chain setup.
                                       For client authentication, CA-certificate-path or CA-certificate-file must be configured.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="verify-depth" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the level of verifying other CA's certificates.  
                        	       According to this value, the server tracks the related CAs for verification.
                        	       If one CA should be verified, it can be set up as 1.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="verify-client" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the client authentication level.
                        [Defined Value]: [0]: Requests no authentication.
                        		 		[1]: The user should show available authentication to the server.
		                        		[2]: The user must show available authentication to the server.
        		                		[3]: The user must show available authentication to the server. However, when the server does not have authentication, the authentication process is not needed.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="fake-basic-auth" type="xs:token">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>fakebasicauth</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: If using FakeBasicAuth option, SSL logs users in based on client certificate. it gets the userid from the certificate, 
                                       and then authenticates user by doing a "fake" login using that userid and a password of 'password'.
                                                If specifying this directive with VerifyClient directive, it saves the log of authentication history.                   
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="required-ciphers" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the ciphers to be used for SSL.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="random-file" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the file to create the seed of the random password generator for SSL.
                        	       If configured, JEUS Web Server creates the seed at booting time to generate the password for SSL.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="random-file-per-conn" type="len256Type">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <origin-name>randomfileperconnection</origin-name>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: Specifies the number of random file to be used to make random seed when establishing the connection via each SSL.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="pass-phrase-dialog" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the method to get the password phrase for acessing the private key files.
                        [Defined Value]: [builtin] : requests the password phrase when botting the web Server.
                                         [exec:/path/to/program] : executes the program and uses its output as the password phrase when booting the web Server.
                                                                   The program can be either an executable or a shell script.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="crypto-device" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the SSL external crypto device type.
                        [Defined Value]: [builtin] : standard builtin implementations
                                          [cswift] : CryptoSwift hardware engine support
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="error-docType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of the error document.
                        [Value Description]: String within 31 characters
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="status" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the HTTP status code
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="url" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the target URL.  The value should be either a full URL or a path relative to the DOCROOT.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="filterType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of a filter.
                        [Hidden]: true
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element name="real-path" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the absolute path for the filter class.
                        [Hidden]: true
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="expiresType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of Expires.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="mimetype" type="len64Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the expiry dates for different MIME types of Response.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="url" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the expiry date for an URL.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="expires-time" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the expiry date (time).
                                       If the required fields are specified, the expiry date will be set in the Response header.
                        [Value Description]: 1) &lt;code>&lt;seconds>
                                                &lt;code>:  'M': file Modification date + &lt;seconds>
                                                         'A': Access time + &lt;seconds>
                                                &lt;seconds>: Integer value
                                                          Frequently used time samples(seconds):
                                                          1 hour : 3600
                                                          1 day : 86400
                                                          1 week : 604800
                                                          1 month : 2419200
                                             2) &lt;base> [plus] {&lt;num> &lt;type>}*
                                                &lt;base>:  'access' or 'now': access time
                                                            'modification': file modification date
                                                [plus]: optional
                                                &lt;num>: integer value
                                                &lt;type>: 'years', 'months', 'weeks', 'days', 'hours', 'minutes',
                                                              'seconds', 'year', 'month', 'week', 'day', 'hour', 'minute' or
                                                              'second'
                        [Example]: ExpiresTime = "A604800",  ExpiresTime = "M3600"
                                           ExpiresTime = "access plus 1 month", ExpiresTime = "now plus 4 weeks", ExpiresTime = "access 1 month 15 days 2 hours"
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="ssl-useType">         
        <xs:sequence>             
            <xs:element name="ssl-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of SSL.  This name must be one of the names registered in 'sslType' element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="false" minOccurs="0" name="ssl-flag" type="xs:boolean">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <enable>Y</enable>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: It is a mandatory item that must be specified when using SSL in JEUS Web Server.
                                                If SSLFLAG is set to 'true', and  SSL will be used in the node, if set to false, SSL will not be used.
                                                The default setting is 'false'.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="tcpgwType">         
        <xs:sequence>             
            <xs:element name="name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the name of TcpGW
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:choice>                 
                <xs:element maxOccurs="unbounded" minOccurs="1" name="port" type="len1024Type">                     
                    <xs:annotation>                         
                        <xs:documentation><![CDATA[
                            [Description]: This element configures up to 100 ports to listen client requests.
                                           Multiple ports can listen simultaneously  but this element cannot be applied together with the 'listen' element.                
                        ]]>
                        </xs:documentation>                     
                    </xs:annotation>                 
                </xs:element>                 
                <xs:element maxOccurs="unbounded" minOccurs="1" name="listen" type="len1024Type">                     
                    <xs:annotation>                         
                        <xs:documentation><![CDATA[
                            [Description]: This element configures up to 100 IP addresses and ports to listen client requests.
                                           It can be used to listen with a specific IP adress for a machine with multiple IP addresses.
                                           This element cannot be applied together with the 'port' element.
                        ]]>
                        </xs:documentation>                     
                    </xs:annotation>                 
                </xs:element>             
            </xs:choice>             
            <xs:element maxOccurs="unbounded" minOccurs="1" name="server-address" type="len1024Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element configures up to 100 server IP addresses and ports to process client requests.
                                       The load will be distributed with the Round Robin method.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="300000" minOccurs="0" name="timeout" type="xs:long">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the connection timeout period of receiving response from the web server.
                        	       If it taks more than this timeout value for a user to receive content, JEUS Web Server disconnects it.
                        [Value Description]: milisecond
                        [Defined Value]: [0]: disables the timeout feature
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="5000" minOccurs="0" name="connect-timeout" type="xs:long">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the timeout period for Web Server TCP Gateway to connect a target server.
                                       If the target server is not responding within this timeout period, the request is forwarded to another server.
                        [Value Description]: milisecond
                        [Defined Value]: [0] : disables the connect-timeout feature.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="access-name" type="len32Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the users who are allowed to access.
                        	       The value must be one of the names in the accessType element.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="RR" minOccurs="0" name="schedule" type="len256Type">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the request handling method for distributing requests to the server processes.
                        [Defined Value]: [RR] : 'Round Robin' distributes a request to an idle server process with Round Robin method.
                                         [FA] : 'First Assign' distributes a request to the server process that has the highest priority among the idle server processes.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="cache-useType">         
        <xs:sequence>             
            <xs:element default="128" minOccurs="0" name="cache-entry" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element stes the total number of hashing key to be used for caching.
                        	       If a large number of hashing key is used it takes more time to find the hashing key but less time to find the value from the hashing key. 
                                       If a small number of hashing key is used it takes less time to find the hashing key but more time to find the value from the hashing key. 
                                       e.g. Using only A and P vs. using A, B, C...Z as hashing keys
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="128" minOccurs="0" name="cache-size" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the cache entry size that can be used for storing session information.
                        [Value Description]: This value sets the size of an cache entry in Kbytes.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="cache-refresh-html" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the cache refresh time for HTML.
                        [Value Description]: second
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="cache-refresh-dir" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the cache refresh time for directory index.
                        [Value Description]: second
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="keep-alive-useType">         
        <xs:sequence>             
            <xs:element default="false" minOccurs="0" name="keep-alive" type="xs:boolean">                 
                <xs:annotation>                     
                    <xs:appinfo>                         <metadata>                             <enable>Y</enable>                         </metadata>                     
                    </xs:appinfo>                     
                    <xs:documentation><![CDATA[
                        [Description]: HTTP 1.1 specification provides KeepAlive for persistent connections. this can increase download speeds, 
                        reduce the latency of many small HTTP connections, optimize network usage, and even reduce server load.
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="60000" minOccurs="0" name="keep-alive-timeout" type="xs:long">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the keep alive timeout period to disconnect the client connections.
                        	       JEUS Web Server will wait for a subsequent request before closing the connection.
                        [Value Description]: milisecond
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element default="0" minOccurs="0" name="keep-alive-max" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: This element sets the maximum number of requests you want Apache to entertain per connection.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:simpleType name="len16Type">         
        <xs:restriction base="xs:token">             
            <xs:maxLength value="16"/>         
        </xs:restriction>     
    </xs:simpleType>      
    <xs:simpleType name="len32Type">         
        <xs:restriction base="xs:token">             
            <xs:maxLength value="32"/>         
        </xs:restriction>     
    </xs:simpleType>      
    <xs:simpleType name="svrElementType">         
        <xs:restriction base="xs:token">             
            <xs:enumeration value="HTML">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: HTML(HyperText Markup Language)
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:enumeration>             
            <xs:enumeration value="CGI">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: CGI(Commom Gateway Inteface)
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:enumeration>             
            <xs:enumeration value="SSI">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: SSI(Server Side Include)
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:enumeration>             
            <xs:enumeration value="JSV">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: JSV(Java SerVlet)
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:enumeration>             
