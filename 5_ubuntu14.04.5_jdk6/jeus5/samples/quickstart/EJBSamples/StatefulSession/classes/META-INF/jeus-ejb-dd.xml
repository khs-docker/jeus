<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jeus-ejb-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <module-info/>
    <beanlist>
        <jeus-bean>
            <ejb-name>counter</ejb-name>
            <export-name>counter</export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>false</local-invoke-optimize>
            <thread-max>200</thread-max>
            <pooling-bean>true</pooling-bean>
            <object-management>
                <bean-pool>
                    <pool-min>4</pool-min>
                    <pool-max>5</pool-max>
                    <resizing-period>1800000</resizing-period>
                </bean-pool>
                <connect-pool>
                    <pool-min>2</pool-min>
                    <pool-max>3</pool-max>
                    <resizing-period>1800000</resizing-period>
                </connect-pool>
                <capacity>2000</capacity>
                <passivation-timeout>20000</passivation-timeout>
                <disconnect-timeout>60000</disconnect-timeout>
            </object-management>
            <file-db-info>
                <local-file-db>
                    <file-db-path>./logz</file-db-path>
                    <file-db-name>sfsb_fdb</file-db-name>
                    <min-hole>1000</min-hole>
                    <packing-rate>0.5</packing-rate>
                </local-file-db>
            </file-db-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
    </beanlist>
</jeus-ejb-dd>
