<?xml version="1.0"?>
<jms-server xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
	<service-config>
	    <name>jmstest</name>
	    <port>29741</port>  
	    <blocking-socket>true</blocking-socket>
	    <use-ssl>false</use-ssl>
	    <connection-timeout>1000</connection-timeout>
	    <client-keepalive-timeout>10</client-keepalive-timeout>
	</service-config>    
	<thread-pool>
		<min>10</min>
		<max>20</max>
	</thread-pool>
	<connection-factory>
		<type>queue</type>
		<name>QueueConnectionFactory</name>
	</connection-factory>
	<connection-factory>
		<type>topic</type>
		<name>TopicConnectionFactory</name>
	</connection-factory>
	<connection-factory>
		<type>topic</type>
		<name>jms/newTopicConnectionFactory</name>
		<client-id>testClient1</client-id>
	</connection-factory>
	<connection-factory>
		<type>topic</type>
		<name>jms/MDBTACCESSTEST_FACTORY</name>
		<client-id>testClient2</client-id>
	</connection-factory>
	<!-- settings for adventure builder -->
	<connection-factory>
		<type>queue</type>
		<name>jms/airline/QueueConnectionFactory</name>
	</connection-factory>
	<connection-factory>
		<type>queue</type>
		<name>jms/activity/QueueConnectionFactory</name>
	</connection-factory>
	<connection-factory>
		<type>queue</type>
		<name>jms/lodging/QueueConnectionFactory</name>
	</connection-factory>
	<connection-factory>
		<type>queue</type>
		<name>jms/opc/QueueConnectionFactory</name>
	</connection-factory>
	<destination>
		<type>queue</type>
		<name>ExamplesQueue</name>
	</destination>
	<destination>
		<type>topic</type>
		<name>ExamplesTopic</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/airline/AirlineQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/activity/ActivityQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/lodging/LodgingQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/opc/WorkFlowMgrQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/opc/OrderFillerQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/opc/CRMQueue</name>
	</destination>
	<destination>
		<type>queue</type>
		<name>jms/opc/WebServiceBrokerQueue</name>
	</destination>
</jms-server>
