<?xml version="1.0" encoding="UTF-8"?>
<jeus-system xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <node>
        <name>qs</name>
        <engine-container>
            <name>container1</name>
            <command-option>-Xms256m -Xmx512m -Djeus.ejb.enable.configDeleteOption=true -Djeus.servlet.context.attribute.serialize=true</command-option>
            <engine-command>
                <type>jms</type>
                <name>engine1</name>
            </engine-command>
            <engine-command>
                <type>ejb</type>
                <name>engine1</name>
                <system-logging>
                    <level>SEVERE</level>
                    <use-parent-handlers>true</use-parent-handlers>
                </system-logging>
            </engine-command>
            <engine-command>
                <type>servlet</type>
                <name>engine1</name>
                <system-logging>
                    <level>SEVERE</level>
                    <use-parent-handlers>true</use-parent-handlers>
                </system-logging>
            </engine-command>
            <sequential-start>true</sequential-start>
            <tm-config>
                <pooling>
                    <min>10</min>
                    <max>20</max>
                    <period>600000</period>
                </pooling>
                <active-timeout>300000</active-timeout>
                <commit-timeout>120000</commit-timeout>
                <capacity>20000</capacity>
            </tm-config>
            <system-logging>
                <level>SEVERE</level>
            </system-logging>
            <invocation-manager-action>AutoClose</invocation-manager-action>
        </engine-container>
        <class-ftp>true</class-ftp>
        <sequential-start>true</sequential-start>
        <enable-jnlp>false</enable-jnlp>
        <enable-webadmin>true</enable-webadmin>
        <system-logging>
            <level>SEVERE</level>
            <handler>
                <console-handler>
                    <name>consoleHandler</name>
                    <level>SEVERE</level>
                </console-handler>
                <file-handler>
                    <name>fileHandler</name>
                    <level>SEVERE</level>
                </file-handler>
            </handler>
        </system-logging>
        <session-server>
            <resolution>10000</resolution>
            <session-manager>
                <name>session1</name>
                <passivation-to>600000</passivation-to>
                <removal-to>3000000</removal-to>
                <check-to>1000</check-to>
            </session-manager>
            <session-manager>
                <name>session2</name>
                <passivation-to>600000</passivation-to>
                <removal-to>3000000</removal-to>
                <check-to>1000</check-to>
            </session-manager>
        </session-server>
    </node>
    <resource>
        <data-source>
            <database>
                <vendor>others</vendor>
                <export-name>adventure</export-name>
                <data-source-class-name>jeus.jdbc.driver.blackbox.BlackboxConnectionPoolDataSource</data-source-class-name>
                <data-source-type>ConnectionPoolDataSource</data-source-type>
                <property>
                    <name>URL</name>
                    <type>java.lang.String</type>
                    <value>jdbc:hsqldb:hsql://localhost/</value>
                </property>
                <property>
                    <name>DriverClassName</name>
                    <type>java.lang.String</type>
                    <value>org.hsqldb.jdbcDriver</value>
                </property>
                <property>
                    <name>User</name>
                    <type>java.lang.String</type>
                    <value>sa</value>
                </property>
                <property>
                    <name>Password</name>
                    <type>java.lang.String</type>
                    <value/>
                </property>
            </database>
            <database>
                <vendor>others</vendor>
                <export-name>SampleDS</export-name>
                <data-source-class-name>jeus.jdbc.driver.blackbox.BlackboxConnectionPoolDataSource</data-source-class-name>
                <data-source-type>ConnectionPoolDataSource</data-source-type>
                <property>
                    <name>URL</name>
                    <type>java.lang.String</type>
                    <value>jdbc:hsqldb:hsql://localhost/</value>
                </property>
                <property>
                    <name>DriverClassName</name>
                    <type>java.lang.String</type>
                    <value>org.hsqldb.jdbcDriver</value>
                </property>
                <property>
                    <name>User</name>
                    <type>java.lang.String</type>
                    <value>sa</value>
                </property>
                <property>
                    <name>Password</name>
                    <type>java.lang.String</type>
                    <value/>
                </property>
            </database>
        </data-source>
        <mail-source>
            <mail-entry>
                <export-name>mail/Session</export-name>
                <mail-property>
                    <name>mail.host</name>
                    <value>nosuchhost.nosuchdomain.com</value>
                </mail-property>
                <mail-property>
                    <name>mail.from</name>
                    <value>nobody@nosuchhost.nosuchdomain.com</value>
                </mail-property>
                <mail-property>
                    <name>mail.user</name>
                    <value>nobody</value>
                </mail-property>
                <mail-property>
                    <name>mail.transport.protocol</name>
                    <value>smtp</value>
                </mail-property>
            </mail-entry>
        </mail-source>
    </resource>
    <application>
