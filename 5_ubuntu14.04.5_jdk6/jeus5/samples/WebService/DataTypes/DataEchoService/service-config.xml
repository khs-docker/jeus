<?xml version="1.0" encoding="UTF-8"?>
<web-services-config xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
	<service>
		<service-name>DataEchoService</service-name>
		<target-namespace>urn:DataEchoService</target-namespace>
		<output-wsdl-file>DataEchoService.wsdl</output-wsdl-file>
		<output-jaxrpc-mapping-file>DataEchoService-mapping.xml</output-jaxrpc-mapping-file>
		<style>wrapped</style>
		<interface>
			<endpoint-interface-class>com.tmax.qa.datatypes.DataEcho</endpoint-interface-class>
			<operation>
			    <java-method-name>echoString</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoBoolean</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoDouble</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoFloat</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoInteger</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoByte</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoBigDecimal</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
			<operation>
			    <java-method-name>echoCalendar</java-method-name>
                	    <soap-action>dataEchoAction</soap-action>
			    <one-way>false</one-way>
			</operation>
		</interface>
	</service>
</web-services-config>
