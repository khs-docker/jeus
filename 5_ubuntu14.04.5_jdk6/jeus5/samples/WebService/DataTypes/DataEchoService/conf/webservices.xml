<?xml version="1.0"?>
<webservices version="1.1" xmlns="http://java.sun.com/xml/ns/j2ee">
    <webservice-description>
        <webservice-description-name>DataEchoService</webservice-description-name>
        <wsdl-file>WEB-INF/wsdl/DataEchoService.wsdl</wsdl-file>
        <jaxrpc-mapping-file>WEB-INF/DataEchoService-mapping.xml</jaxrpc-mapping-file>
        <port-component>
            <port-component-name>DataEchoPort</port-component-name>
            <wsdl-port xmlns:ns2="urn:DataEchoService">ns2:DataEchoPort</wsdl-port>
            <service-endpoint-interface>com.tmax.qa.datatypes.DataEcho</service-endpoint-interface>
            <service-impl-bean>
                <servlet-link>DataEchoServlet</servlet-link>
            </service-impl-bean>
        </port-component>
    </webservice-description>
</webservices>

