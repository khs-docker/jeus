<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jeus-ejb-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <beanlist>
        <jeus-bean>
            <ejb-name>CustomerBean</ejb-name>
            <export-name>ejb.ejbql.relationQuery</export-name>
            <local-export-name>customerLocal</local-export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>false</local-invoke-optimize>
            <thread-max>200</thread-max>
            <pooling-bean>false</pooling-bean>
            <object-management>
                <bean-pool>
                    <pool-min>6</pool-min>
                    <pool-max>200</pool-max>
                    <resizing-period>1800000</resizing-period>
                </bean-pool>
                <connect-pool>
                    <pool-min>6</pool-min>
                    <pool-max>200</pool-max>
                    <resizing-period>1800000</resizing-period>
                </connect-pool>
                <capacity>2000</capacity>
                <passivation-timeout>100000</passivation-timeout>
                <disconnect-timeout>1800000</disconnect-timeout>
            </object-management>
            <persistence-optimize>
                <engine-type>EXCLUSIVE_ACCESS</engine-type>
                <non-modifying-method>
                    <method-name>getName</method-name>
                </non-modifying-method>
                <entity-cache-size>0</entity-cache-size>
                <update-delay-till-tx>true</update-delay-till-tx>
            </persistence-optimize>
            <schema-info>
                <table-name>Customer_tbl</table-name>
                <cm-field>
                    <field>id</field>
                </cm-field>
                <cm-field>
                    <field>name</field>
                </cm-field>
                <creating-table>
                    <use-existing-table/>
                </creating-table>
                <deleting-table>true</deleting-table>
                <db-vendor>hsql</db-vendor>
                <data-source-name>datasource1</data-source-name>
            </schema-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
        <jeus-bean>
            <ejb-name>ReservationBean</ejb-name>
            <export-name>reservation</export-name>
            <local-export-name>reservationLocal</local-export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>false</local-invoke-optimize>
            <thread-max>200</thread-max>
            <pooling-bean>false</pooling-bean>
            <object-management>
                <bean-pool>
                    <pool-min>6</pool-min>
                    <pool-max>200</pool-max>
                    <resizing-period>1800000</resizing-period>
                </bean-pool>
                <connect-pool>
                    <pool-min>6</pool-min>
                    <pool-max>200</pool-max>
                    <resizing-period>1800000</resizing-period>
                </connect-pool>
                <capacity>2000</capacity>
                <passivation-timeout>100000</passivation-timeout>
                <disconnect-timeout>1800000</disconnect-timeout>
            </object-management>
            <persistence-optimize>
                <engine-type>EXCLUSIVE_ACCESS</engine-type>
                <non-modifying-method>
                    <method-name>getPayAmount</method-name>
                </non-modifying-method>
                <non-modifying-method>
                    <method-name>getDate</method-name>
                </non-modifying-method>
                <entity-cache-size>0</entity-cache-size>
                <update-delay-till-tx>true</update-delay-till-tx>
            </persistence-optimize>
            <schema-info>
                <table-name>Reservation_tbl</table-name>
                <cm-field>
                    <field>id</field>
                </cm-field>
                <cm-field>
                    <field>payAmount</field>
                </cm-field>
                <cm-field>
                    <field>reservedDate</field>
                </cm-field>
                <creating-table>
                    <use-existing-table/>
                </creating-table>
                <deleting-table>true</deleting-table>
                <db-vendor>hsql</db-vendor>
                <data-source-name>datasource1</data-source-name>
            </schema-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
    </beanlist>
    <ejb-relation-map>
        <relation-name>Customer-Reservation</relation-name>
        <table-name>Customer_Reservation_Link</table-name>
        <jeus-relationship-role>
            <relationship-role-name>Reservation-has-many-Customers</relationship-role-name>
            <column-map>
                <foreign-key-column>reservation</foreign-key-column>
                <target-primary-key-column>id</target-primary-key-column>
            </column-map>
        </jeus-relationship-role>
        <jeus-relationship-role>
            <relationship-role-name>Customer-has-many-Reservations</relationship-role-name>
            <column-map>
                <foreign-key-column>customer</foreign-key-column>
                <target-primary-key-column>id</target-primary-key-column>
            </column-map>
        </jeus-relationship-role>
    </ejb-relation-map>
</jeus-ejb-dd>

