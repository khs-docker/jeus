<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jeus-ejb-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <module-info/>
    <beanlist>
        <jeus-bean>
            <ejb-name>otmtest</ejb-name>
            <export-name>otmtest</export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>true</local-invoke-optimize>
            <pooling-bean>false</pooling-bean>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
        <jeus-bean>
            <ejb-name>teacher</ejb-name>
            <local-export-name>local/Teacher</local-export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>true</local-invoke-optimize>
            <pooling-bean>false</pooling-bean>
            <persistence-optimize>
                <engine-type>SINGLE_OBJECT</engine-type>
                <entity-cache-size>0</entity-cache-size>
                <update-delay-till-tx>true</update-delay-till-tx>
            </persistence-optimize>
            <schema-info>
                <table-name>OTM_TEACHER</table-name>
                <cm-field>
                    <field>name</field>
                    <column-name>TEACHER_NAME</column-name>
                </cm-field>
                <cm-field>
                    <field>teacherid</field>
                    <column-name>TEACHER_ID</column-name>
                </cm-field>
                <creating-table>
                    <use-existing-table/>
                </creating-table>
                <deleting-table>true</deleting-table>
                <db-vendor>oracle</db-vendor>
                <data-source-name>localxa</data-source-name>
            </schema-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
        <jeus-bean>
            <ejb-name>student</ejb-name>
            <local-export-name>local/Student</local-export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>true</local-invoke-optimize>
            <pooling-bean>false</pooling-bean>
            <persistence-optimize>
                <engine-type>SINGLE_OBJECT</engine-type>
                <entity-cache-size>0</entity-cache-size>
                <update-delay-till-tx>true</update-delay-till-tx>
            </persistence-optimize>
            <schema-info>
                <table-name>OTM_STUDENT</table-name>
                <creating-table>
                    <use-existing-table/>
                </creating-table>
                <deleting-table>true</deleting-table>
                <db-vendor>oracle</db-vendor>
                <data-source-name>localxa</data-source-name>
            </schema-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
    </beanlist>
    <ejb-relation-map>
        <relation-name>student-teacher</relation-name>
        <jeus-relationship-role>
            <relationship-role-name>studentRelationshipRole</relationship-role-name>
            <column-map>
                <foreign-key-column>FK_TEACHER_ID</foreign-key-column>
                <target-primary-key-column>TEACHER_ID</target-primary-key-column>
            </column-map>
        </jeus-relationship-role>
    </ejb-relation-map>
</jeus-ejb-dd>

