<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <display-name>mtm</display-name>
  <enterprise-beans>
    <entity>
      <display-name>Student</display-name>
      <ejb-name>Student</ejb-name>
      <home>ejb.relation.mtom.StudentRemoteHome</home>
      <remote>ejb.relation.mtom.StudentRemote</remote>
      <local-home>ejb.relation.mtom.StudentLocalHome</local-home>
      <local>ejb.relation.mtom.StudentLocal</local>
      <ejb-class>ejb.relation.mtom.StudentBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>StudentLocal</abstract-schema-name>
      <cmp-field>
        <field-name>number</field-name>
      </cmp-field>
      <cmp-field>
        <field-name>name</field-name>
      </cmp-field>
      <primkey-field>number</primkey-field>
    </entity>
    <entity>
      <display-name>CourseLocal</display-name>
      <ejb-name>Course</ejb-name>
      <home>ejb.relation.mtom.CourseRemoteHome</home>
      <remote>ejb.relation.mtom.CourseRemote</remote>
      <local-home>ejb.relation.mtom.CourseLocalHome</local-home>
      <local>ejb.relation.mtom.CourseLocal</local>
      <ejb-class>ejb.relation.mtom.CourseBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-version>2.x</cmp-version>
      <abstract-schema-name>CourseLocal</abstract-schema-name>
      <cmp-field>
        <field-name>code</field-name>
      </cmp-field>
      <cmp-field>
        <field-name>subject</field-name>
      </cmp-field>
      <primkey-field>code</primkey-field>
    </entity>
    <session>
      <display-name>mtmtest</display-name>
      <ejb-name>mtmtest</ejb-name>
      <home>ejb.relation.mtom.mtmtestHome</home>
      <remote>ejb.relation.mtom.mtmtest</remote>
      <ejb-class>ejb.relation.mtom.mtmtestBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Bean</transaction-type>
    </session>
  </enterprise-beans>
  <relationships>
    <ejb-relation>
      <ejb-relation-name>student-course</ejb-relation-name>
      <ejb-relationship-role>
        <description>student</description>
        <ejb-relationship-role-name>StudentRelationshipRole</ejb-relationship-role-name>
        <multiplicity>Many</multiplicity>
        <relationship-role-source>
          <description>student</description>
          <ejb-name>Student</ejb-name>
        </relationship-role-source>
        <cmr-field>
          <description>course</description>
          <cmr-field-name>course</cmr-field-name>
          <cmr-field-type>java.util.Collection</cmr-field-type>
        </cmr-field>
      </ejb-relationship-role>
      <ejb-relationship-role>
        <description>course</description>
        <ejb-relationship-role-name>CourseRelationshipRole</ejb-relationship-role-name>
        <multiplicity>Many</multiplicity>
        <relationship-role-source>
          <description>course</description>
          <ejb-name>Course</ejb-name>
        </relationship-role-source>
        <cmr-field>
          <description>student</description>
          <cmr-field-name>student</cmr-field-name>
          <cmr-field-type>java.util.Collection</cmr-field-type>
        </cmr-field>
      </ejb-relationship-role>
    </ejb-relation>
  </relationships>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>Student</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
    <container-transaction>
      <method>
        <ejb-name>Course</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
