<?xml version="1.0"?>
<!DOCTYPE tool-option PUBLIC "-//Tmax Soft., Inc.//DTD JEUS Tool Option 4.0//EN"
                             "http://www.tmaxsoft.com/jeus/dtd/4.0/tool-option.dtd">
<tool-option>
    <xml-option>
        <xml-parsing>
            <enable-validation>false</enable-validation>
            <local-dtd-validation>true</local-dtd-validation>
            <ignore-element-order>true</ignore-element-order>
        </xml-parsing>
        <xml-writing>
            <show-default>true</show-default>
            <force-default-dtd>false</force-default-dtd>
            <default-dtd-dir>http://www.tmaxsoft.com/jeus/dtd/4.0/</default-dtd-dir>
        </xml-writing>
    </xml-option>
    <boot-option>
        <dynamic>false</dynamic>
        <xml-boot>true</xml-boot>
    </boot-option>
    <ejb-option>
        <fast-reload>false</fast-reload>
    </ejb-option>
</tool-option>
