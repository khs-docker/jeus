<?xml version="1.0" encoding="Shift-JIS"?>
<xs:schema 
    attributeFormDefault="unqualified" 
    elementFormDefault="qualified" 
    targetNamespace="http://www.tmaxsoft.com/xml/ns/jeus" version="5.0" 
    xmlns="http://www.tmaxsoft.com/xml/ns/jeus" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema">      
    <xs:include schemaLocation="jeus-common.xsd"/>      
    <xs:element name="security-manager" type="security-managerType">         
        <xs:annotation>             
            <xs:documentation><![CDATA[
                        [Description]: security-manager elementは、JEUS securityの情報管理に対する説明です。
                    ]]>
            </xs:documentation>         
        </xs:annotation>     
    </xs:element>      
    <xs:complexType name="security-managerType">         
        <xs:sequence>             
            <xs:element default="true" minOccurs="0" name="use-nio" type="xs:boolean">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: security manager Nonblocking I/Oを使用して通信するかどうかを指定します。
                        [Performance Recommendation]:　数多いエンジン・コンテナおよびクライアントを使用する場合には
                        Nonblocking I/Oがより高い効率を示し、security managerを使用するクライアントの数が少なければ、
                        Blocking I/Oがより効率的です。
                    ]]>
                    </xs:documentation>                     
                    <xs:appinfo>                         <metadata>                             <element-level>ADVANCED</element-level>                         </metadata>                     
                    </xs:appinfo>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="connect-retries" type="xs:int">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: Security Network serviceの中でconnection retry count値を定義する
                     
                    ]]>
                    </xs:documentation>                     
                    <xs:appinfo>                         <metadata>                             <element-level>ADVANCED</element-level>                         </metadata>                     
                    </xs:appinfo>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="security-domains" type="security-domainsType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: このJEUS Managerで使用するsecurity domainを設定します。詳しくは、
                        JEUSセキュリティガイドを参照してください。
                     
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>      
    <xs:complexType name="security-domainsType">         
        <xs:sequence>             
            <xs:element maxOccurs="unbounded" minOccurs="0" name="security-domain" type="security-domainType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: JEUSで使用するsecurity domainを設定します。
                        
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>         
        </xs:sequence>     
    </xs:complexType>     
    <xs:complexType name="security-domainType">         
        <xs:sequence>             
            <xs:element name="name" type="xs:token">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                            [Description]: ドメインの名前を設定します。
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="cache-manager" type="cache-serviceType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: JEUS Security Framwrok?? ???? cache?? ??? ?.
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:element minOccurs="0" name="keystore-manager" type="keystore-managerType">                 
                <xs:annotation>                     
                    <xs:documentation><![CDATA[
                        [Description]: keystore/truststore fileに対する設定をする
                    
                    ]]>
                    </xs:documentation>                 
                </xs:annotation>             
            </xs:element>             
            <xs:choice>                 
                <xs:group ref="SecurityServices"/>                 
                <xs:element maxOccurs="unbounded" minOccurs="0" name="custom-service" type="SecurityServiceType">                     
                    <xs:annotation>                         
                        <xs:documentation><![CDATA[
                        [Description]: 該当ドメインにJEUS Security Frameworkで提供するサービスの属性に関係なく、
                                       JEUS Security SPIを内装したサービスの属性に別途のセキュリティサービスを登録することができます。
                   
                    ]]>
                        </xs:documentation>                     
                    </xs:annotation>                 
                </xs:element>             
            </xs:choice>         
        </xs:sequence>     
    </xs:complexType>      
