<?xml version="1.0"?>
<jms-server xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <service-config>
        <name>jmstest</name>
        <port>19741</port>  
        <blocking-socket>true</blocking-socket>
        <use-ssl>false</use-ssl>
        <connection-timeout>1000</connection-timeout>
        <client-keepalive-timeout>10</client-keepalive-timeout>
    </service-config>    
	<thread-pool>
            <min>10</min>
            <max>20</max>
	</thread-pool>
    <connection-factory>
        <type>queue</type>
        <name>QueueConnectionFactory</name>
    </connection-factory>
    <connection-factory>
        <type>topic</type>
        <name>TopicConnectionFactory</name>
    </connection-factory>
    <destination>
        <type>queue</type>
        <name>ExamplesQueue</name>
    </destination>
    <destination>
        <type>topic</type>
        <name>ExamplesTopic</name>
    </destination>
</jms-server>
