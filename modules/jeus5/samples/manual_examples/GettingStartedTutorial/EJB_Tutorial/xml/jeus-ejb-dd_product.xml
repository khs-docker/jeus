<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jeus-ejb-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <module-info/>
    <beanlist>
        <jeus-bean>
            <ejb-name>product</ejb-name>
            <export-name>ProductBean</export-name>
            <export-port>0</export-port>
            <single-vm-only>false</single-vm-only>
            <local-invoke-optimize>true</local-invoke-optimize>
            <thread-max>100</thread-max>
            <pooling-bean>false</pooling-bean>
            <persistence-optimize>
                <engine-type>EXCLUSIVE_ACCESS</engine-type>
                <non-modifying-method>
                    <method-name>getDescription</method-name>
                </non-modifying-method>
                <non-modifying-method>
                    <method-name>getPrice</method-name>
                </non-modifying-method>
                <entity-cache-size>0</entity-cache-size>
                <update-delay-till-tx>true</update-delay-till-tx>
            </persistence-optimize>
            <schema-info>
                <table-name>product</table-name>
                <cm-field>
                    <field>productID</field>
                    <column-name>productID</column-name>
                </cm-field>
                <cm-field>
                    <field>description</field>
                    <column-name>description</column-name>
                </cm-field>
                <cm-field>
                    <field>price</field>
                </cm-field>
                <creating-table>
                    <use-existing-table/>
                </creating-table>
                <deleting-table>false</deleting-table>
                <find-method>
                    <query-method>
                        <method-name>findByDescription</method-name>
                        <method-params>
                            <method-param>java.lang.String</method-param>
                        </method-params>
                    </query-method>
                    <sql>description = ?</sql>
                </find-method>
                <find-method>
                    <query-method>
                        <method-name>findByPrimaryKey</method-name>
                        <method-params>
                            <method-param>pkproduct.Pid</method-param>
                        </method-params>
                    </query-method>
                </find-method>
                <find-method>
                    <query-method>
                        <method-name>findInRange</method-name>
                        <method-params>
                            <method-param>double</method-param>
                            <method-param>double</method-param>
                        </method-params>
                    </query-method>
                    <sql>price between ? and ?</sql>
                </find-method>
                <db-vendor>oracle</db-vendor>
                <data-source-name>datasource1</data-source-name>
            </schema-info>
            <database-insert-delay>ejbPostCreate</database-insert-delay>
            <cm-persistence-optimize>
                <subengine-type>ReadLocking</subengine-type>
                <fetch-size>10</fetch-size>
                <init-caching>false</init-caching>
            </cm-persistence-optimize>
            <enable-instant-ql>false</enable-instant-ql>
            <max-message>10</max-message>
        </jeus-bean>
    </beanlist>
</jeus-ejb-dd>

