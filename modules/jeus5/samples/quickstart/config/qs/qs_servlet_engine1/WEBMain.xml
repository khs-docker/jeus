<?xml version="1.0"?>
<web-container xmlns="http://www.tmaxsoft.com/xml/ns/jeus">
    <monitoring>
        <check-thread-pool>300000</check-thread-pool>
        <check-class-reload>300000</check-class-reload>
        <check-session>300000</check-session>
    </monitoring>
    <redirect-stdout>false</redirect-stdout>
    <redirect-stderr>false</redirect-stderr>
    <shutdown-timeout>-1</shutdown-timeout>
    <context-group>
        <group-name>MyGroup</group-name>
        <webserver-connection>
            <http-listener>
                <listener-id>http1</listener-id>
                <port>8088</port>
                <output-buffer-size>8192</output-buffer-size>
                <thread-pool>
                    <min>10</min>
                    <max>20</max>
                    <step>1</step>
                    <max-idle-time>300000</max-idle-time>
                    <max-wait-queue>4</max-wait-queue>
                    <max-queue>-1</max-queue>
                </thread-pool>
                <postdata-read-timeout>30000</postdata-read-timeout>
                <back-log>50</back-log>
                <server-access-control>false</server-access-control>
            </http-listener>
        </webserver-connection>
        <session-config>
            <shared>false</shared>
            <persistent>false</persistent>
        </session-config>
        <print-error-to-browser>false</print-error-to-browser>
        <jsp-engine>
            <keep-generated>true</keep-generated>
            <check-included-jspfile>false</check-included-jspfile>
        </jsp-engine>
        <response-header>
            <session-id-cookie>
                <version>0</version>
                <path>/</path>
                <max-age>-1</max-age>
                <secure>false</secure>
            </session-id-cookie>
        </response-header>
    </context-group>
    <session-cluster>
        <session-routing>false</session-routing>
    </session-cluster>
</web-container>
